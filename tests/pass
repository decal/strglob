#!/usr/bin/env bash

# Each one of these invocation should have a zero return code..

set -x

./strglob ''
./strglob 'QQ'
./strglob '[1-3]'
./strglob '[1-03]'
./strglob '[01-03]'
./strglob '[1-3]QQ'
./strglob '[1-3]QQ[4-5]'
./strglob 'QQ[4-5]'
./strglob '[1-2]QQ{4,5}'
./strglob ' [1-2]QQ[4-5]'
./strglob '[1-2][4-5]'
./strglob '[1-2]{a,b,c}'
./strglob 'Q[1-2]QQ[4-5]'
./strglob '{a,b,c}'
./strglob 'QQ{a,b,c}'
./strglob '{a,b,c}QQ'
./strglob 'QQ{a,b,c}QQ'
./strglob '{a,b,c}{x,y,z}'
./strglob '{a,b,c}QQ{x,y,z}'
./strglob 'QQ{a,b,c}QQ{x,y,z}'
./strglob '{a,b,c}QQ{x,y,z}QQ'
./strglob 'QQ{a,b,c}QQ{x,y,z}QQ'
./strglob '{{a,b}'
./strglob '{{a,b}}'
./strglob '{{a,b}}}'
./strglob '[-1-0]'
./strglob '[-1-1]'
./strglob '[100-102]'
./strglob '[00-03]'
./strglob '[000-03]'
./strglob '[-000-03]'
./strglob '[+000-03]'
./strglob '[+1-+2]'
./strglob '[1-+2]'
./strglob '[+1-2]'
./strglob '[+2-1]'
./strglob '[2-+1]'
./strglob '[-2-1]'
./strglob '[-2-+1]'
./strglob '[-2--4]'
./strglob '[-4--2]'
./strglob '[1--2]'
./strglob '[+1--2]'
./strglob '[c-a]'
./strglob '[a-c]'
./strglob '[x-}]'
./strglob '[\`-Z]'
./strglob '{x..z}'
./strglob '{a..c..1}'
./strglob '{d..f..5}'
./strglob '{0..2}'
./strglob '{0..2..1}'
./strglob '{+0..2..1}'
./strglob '{+0..2..+1}'
./strglob '{+0..+2..+1}'
./strglob '{+0..-2..+1}'
./strglob '{-0..-2..1}'
./strglob '{-1..-3..-1}'
./strglob '{1..-3..1}'
./strglob '{-1..1..+1}'
./strglob '{1..4..2.2}'
./strglob '{1..4.8..2.2}'
./strglob '{1.2..4.8..2.2}'
./strglob '{/etc/issue.net}'
./strglob '{$LOGNAME}'

exit 0
